<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.usjt.beehealthy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/bee_healthy_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activities.Nutritionist.ui.articles.ArticleDetails"
            android:label="@string/title_activity_article" />
        <activity
            android:name=".Activities.Nutritionist.ui.articles.ArticleAddActivity"
            android:label="@string/title_activity_article_add"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.Nutritionist.ui.client.NutritionalPlanAddActivity"
            android:label="@string/title_activity_nutritionist_plan_add" />
        <activity
            android:name=".Activities.Nutritionist.ui.client.NutritionistPlanDetail"
            android:label="@string/title_activity_nutritionist_plan_detail" />
        <activity
            android:name=".Activities.Nutritionist.ui.client.NutritionalPlanActivity"
            android:background="@color/orange"
            android:label="@string/title_activity_plan_list" />
        <activity
            android:name=".Activities.Nutritionist.ui.client.ClientDetails"
            android:label="@string/title_activity_client_details"
            android:theme="@style/AppTheme.NoActionBar.NoActionBar" />
        <activity
            android:name=".Activities.Register.RegisterActivity"
            android:label="@string/title_activity_register" />
        <activity
            android:name=".Activities.Nutritionist.NutritionistMenu"
            android:label="@string/title_activity_nutritionist_menu" />
        <activity
            android:name=".Activities.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Configuracoes"></activity>
        <activity android:name=".AlterarPaciente" />
        <activity android:name=".ConsultasPaciente" />
        <activity android:name=".ConsultaM" />
        <activity android:name=".RegistraNutri" />
        <activity android:name=".MarcarConsulta" />
        <activity android:name=".Consultas" />
        <activity android:name=".Register" />
        <activity android:name=".PerfilNutricionista" />
        <activity android:name=".Menu" />
    </application>

</manifest>